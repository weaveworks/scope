{
  "name": "weave-scope",
  "version": "1.2.0",
  "description": "SPA JS app for Weave Scope visualising the application network.",
  "repository": "weaveworks/scope",
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "d3": "~3.5.5",
    "dagre": "0.7.4",
    "debug": "~2.2.0",
    "flux": "2.0.3",
    "font-awesome": "4.3.0",
    "font-awesome-webpack": "0.0.3",
    "immutable": "~3.7.4",
    "keymirror": "0.1.1",
    "lodash": "~3.9.3",
    "material-ui": "0.11.0",
    "materialize-css": "0.96.1",
    "object-assign": "2.0.0",
    "page": "1.6.3",
    "react": "~0.13.3",
    "react-motion": "0.2.7",
    "react-tap-event-plugin": "0.1.7",
    "reqwest": "~1.1.5",
    "timely": "0.1.0"
  },
  "devDependencies": {
    "autoprefixer-core": "5.2.0",
    "babel-core": "5.4.7",
    "babel-eslint": "3.1.9",
    "babel-jest": "5.3.0",
    "babel-loader": "5.1.3",
    "css-loader": "0.14.4",
    "eslint": "~0.21.2",
    "eslint-loader": "0.11.2",
    "eslint-plugin-jasmine": "1.0.0",
    "eslint-plugin-react": "2.3.0",
    "file-loader": "0.8.4",
    "jest-cli": "~0.4.19",
    "json-loader": "0.5.2",
    "less": "~2.5.1",
    "less-loader": "2.2.0",
    "postcss-loader": "0.4.3",
    "style-loader": "0.12.3",
    "url": "0.10.3",
    "url-loader": "0.5.6",
    "webpack": "~1.9.10"
  },
  "optionalDependencies": {
    "express": "~4.13.3",
    "express-http-proxy": "~0.6.0",
    "proxy-middleware": "~0.13.1",
    "react-hot-loader": "~1.2.8",
    "webpack-dev-server": "~1.10.1"
  },
  "scripts": {
    "build": "webpack -p --config webpack.production.config.js",
    "start": "node server.js",
    "start-production": "NODE_ENV=production node server.js",
    "test": "jest --coverage",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint": "npm run lint-code && npm run lint-tests",
    "lint-code": "eslint app",
    "lint-tests": "eslint --ignore-path .eslintignore-test --env jest app/**/*-test.js",
    "clean": "rm build/app.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "immutable",
      "d3",
      "dagre",
      "keymirror",
      "object-assign",
      "lodash",
      "debug"
    ]
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
